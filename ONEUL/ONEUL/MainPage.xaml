<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ONEUL.MainPage" BarBackgroundColor="#E88346" BarTextColor="White">

    <ContentPage Title="타임라인">
        <StackLayout Orientation="Vertical">
            <Frame BackgroundColor="#E88346" Padding="15">
                <Label Text="TimeLine" FontSize="23" TextColor="White"/>
            </Frame>
            <ListView x:Name="main_list">
                <ListView.ItemsSource/>
            </ListView>
        </StackLayout>
    </ContentPage>

    <ContentPage Title="통계">
        <StackLayout Orientation="Vertical">
            <Frame BackgroundColor="#E88346" Padding="15">
                <Label Text="Report" FontSize="23" TextColor="White"/>
            </Frame>
        </StackLayout>
    </ContentPage>

    <ContentPage Title="설정">
        <StackLayout Orientation="Vertical">
            <Frame BackgroundColor="#E88346" Padding="15">
                <Label Text="Setting" FontSize="23" TextColor="White"/>
            </Frame>
        </StackLayout>
    </ContentPage>

</TabbedPage>
    
